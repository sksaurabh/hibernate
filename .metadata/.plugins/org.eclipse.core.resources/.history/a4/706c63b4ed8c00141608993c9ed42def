package com.om.saurabh.hib.dto;

public class Bike extends Vehicle {
	private boolean kick;
	private int cc;
	private int id;
	private String name;
	private String type;
	private String color;

	public boolean isKick() {
		return kick;
	}

	public void setKick(boolean kick) {
		this.kick = kick;
	}

	public int getCc() {
		return cc;
	}

	public void setCc(int cc) {
		this.cc = cc;
	}
}
